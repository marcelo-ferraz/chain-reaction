<?xml version="1.0"?>
<configuration>
  <configSections>
    <section name="liteInjection" type="LiteContainer.Configuration.AppConfig.LiteInjectionSection, LiteContainer"/>
  </configSections>
  <liteInjection>
    <dispatchedGroup>
        <add groupName="" injectType="CertiSign.LCRVerifier.Core.Log.ILogCLR, CertiSign.LCRVerifier.Core">
          <add name="defaultLogInFile" type="CertiSign.LCRVerifier.Core.Log.DefaultCRLLogger, CertiSign.LCRVerifier.Core"/>
          <add name="logInTextbox" type="CertiSign.LCRVerifier.Viewer.Log.TextBoxLogger, CertiSign.LCRVerifier.Viewer"/>
          <add name="logInChart" type="CertiSign.LCRVerifier.Log.ImageChartLogger, CertiSign.LCRVerifier.Viewer"/>
        </add>        
      </dispatchedGroup>

  </liteInjection>
<startup><supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/></startup></configuration>
